{
    "name": "deltablue/laravel-varnish",
    "description": "Making Varnish and Laravel play nice together",
    "keywords": [
        "deltablue",
        "laravel-varnish",
        "laravel",
        "varnish",
        "remote"
    ],
    "homepage": "https://github.com/deltablue-cloud/laravel-varnish",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Mathias Aerts",
            "email": "mathias@delta.blue",
            "homepage": "https://delta.blue",
            "role": "Cloud Platform Engineer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/http": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "DeltaBlue\\Varnish\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeltaBlue\\Varnish\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DeltaBlue\\Varnish\\VarnishServiceProvider"
            ]
        }
    }
}
